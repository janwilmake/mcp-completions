{
  "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
  "name": "mcp-chat-interface",
  "main": "worker.ts",
  "compatibility_date": "2025-12-16",
  "observability": {
    "enabled": true
  },
  "env": {
    "prod": {
      "routes": [{ "custom_domain": true, "pattern": "mcpchat.wilmake.com" }],
      "durable_objects": {
        "bindings": [
          {
            "name": "OAuthProviders",
            "class_name": "OAuthProviders"
          }
        ]
      },
      "migrations": [
        {
          "tag": "v1",
          "new_sqlite_classes": ["OAuthProviders"]
        }
      ],
      "kv_namespaces": [
        {
          "binding": "OPENAI_KEYS",
          "id": "4027568435684af29dcd2e0ffffc93c7"
        }
      ]
    }
  },
  "kv_namespaces": [
    {
      "binding": "OPENAI_KEYS",
      "id": "4027568435684af29dcd2e0ffffc93c7"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "OAuthProviders",
        "class_name": "OAuthProviders"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["OAuthProviders"]
    }
  ]
}
